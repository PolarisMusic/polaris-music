{
  "name": "polaris-backend",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "node --env-file=.env src/api/index.js",
    "processor": "node --env-file=.env src/indexer/runProcessor.js",
    "db:clear": "node --env-file=.env scripts/clearGraphData.js",
    "db:load-white-album": "node --env-file=.env scripts/loadWhiteAlbumData.js",
    "db:reset": "npm run db:clear && npm run db:load-white-album",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=test/jest.config.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=test/jest.config.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=test/jest.config.js --coverage",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=test/jest.config.js --testPathPattern=integration",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=test/jest.config.js --testPathPattern=e2e",
    "test:performance": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=test/jest.config.js --testPathPattern=performance",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=test/jest.config.js --ci --coverage --maxWorkers=2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "eosjs": "^22.0.0",
    "express": "^4.19.0",
    "express-graphql": "^0.12.0",
    "fast-json-stable-stringify": "^2.1.0",
    "graphql": "^15.8.0",
    "ioredis": "^5.4.1",
    "ipfs-http-client": "^60.0.1",
    "neo4j-driver": "^5.23.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  }
}
